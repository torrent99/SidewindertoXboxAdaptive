 413:3DPro.c       **** 
 414:3DPro.c       **** // Reboot converter. Kill USB and wait for the watchdog to catch us.
 415:3DPro.c       **** // Note: the watchdog is unleashed in main() after init_hw() returns.
 416:3DPro.c       **** 
 417:3DPro.c       **** static void FA_NORETURN( reboot ) ( void )
 418:3DPro.c       **** {
 419:3DPro.c       ****     cli() ;					// Disable interrupts
 420:3DPro.c       **** 
 421:3DPro.c       ****     UDCON  = _B1(DETACH) ;
 422:3DPro.c       ****     USBCON = _B0(USBE) | _B1(FRZCLK) ;		// Kill USB
 423:3DPro.c       **** 
 424:3DPro.c       ****     for ( ;; )					// Wait for watchdog to bite (.5s)
 425:3DPro.c       **** 	;
 426:3DPro.c       **** }
