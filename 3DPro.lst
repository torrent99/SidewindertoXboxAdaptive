 281:3DPro.c       **** 
 282:3DPro.c       **** // Initialize FFP/PP by reading the ID. This has to be done, else the PP will
 283:3DPro.c       **** // only use B0/1 for data transfer. Actually only the 3D Pro Plus does that.
 284:3DPro.c       **** // Do it anyways for type determination.
 285:3DPro.c       **** // QueryFFP() needs to be called before this function for a valid sw_clkcnt.
 286:3DPro.c       **** 
 287:3DPro.c       **** static uint8_t InitFFPro ( void )
 288:3DPro.c       **** {
 289:3DPro.c       ****     uint8_t
 290:3DPro.c       ****         butt,
 291:3DPro.c       ****         idpktsz ;
 292:3DPro.c       **** 
 293:3DPro.c       ****     butt = BUTPIN ;				// Save current button data
 294:3DPro.c       **** 
 295:3DPro.c       ****     idpktsz = sw_clkcnt ;			// remember packetsize
 296:3DPro.c       **** 
 297:3DPro.c       ****     QueryFFP( -(sw_clkcnt - 6), 126 ) ;		// Read ID, signal kick at -6 clk of data packet
 298:3DPro.c       **** 
 299:3DPro.c       ****     Delay_1024( T0DEL8MS ) ;			// Give the stick some time..
 300:3DPro.c       **** 
 301:3DPro.c       ****     idpktsz = sw_clkcnt - idpktsz ;		// Sizeof ID packet
 302:3DPro.c       **** 
 303:3DPro.c       ****     if ( ! QueryFFP( 0, DATSZFFP ) )		// Signal for a regular packet, 16 triplets
 304:3DPro.c       **** 	return ( FALSE ) ;			// Timed out..
 305:3DPro.c       **** 
 306:3DPro.c       ****     if ( CheckFFPPkt( ffp_packet ) )		// Packet parity checks out
 307:3DPro.c       ****     {
 308:3DPro.c       **** 	if ( idpktsz == IDSZPP )
 309:3DPro.c       **** 	    sw_id = SW_ID_PP ;			// Found PP
 310:3DPro.c       **** 	else
 311:3DPro.c       **** 	if ( idpktsz == IDSZFFP )
 312:3DPro.c       **** 	    sw_id = SW_ID_FFP ;			// Found FFP
 313:3DPro.c       **** 	else
 314:3DPro.c       **** 	if ( idpktsz == 0 )			// 10nF timer, FFP or PP in 3bit mode
 315:3DPro.c       **** 	{
 316:3DPro.c       **** 	    if ( (butt & BUTMSK) == BUTMSK )	// PP
 317:3DPro.c       **** 		sw_id = SW_ID_PP ;
 318:3DPro.c       **** 	    else				// FFP
 319:3DPro.c       **** 		sw_id = SW_ID_FFP ;
 320:3DPro.c       **** 	}
 321:3DPro.c       **** 	else
 322:3DPro.c       **** 	    return ( FALSE ) ;			// Unknown stick..
 323:3DPro.c       **** 
 324:3DPro.c       **** 	CopyFFPData( ffp_packet ) ;		// Copy data to report buffer
 325:3DPro.c       **** 
 326:3DPro.c       **** 	return ( TRUE ) ;			// Got it
 327:3DPro.c       ****     }
 328:3DPro.c       **** 
 329:3DPro.c       ****     return ( FALSE ) ;				// Stick not found
 330:3DPro.c       **** }
 331:3DPro.c       **** 
