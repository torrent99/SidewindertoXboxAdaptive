 333:3DPro.c       **** 
 334:3DPro.c       **** // Initialize 3D Pro by setting it into digital mode followed by reading the ID.
 335:3DPro.c       **** // This has to be done, else the 3DPro will only use B0/1 for data transfer.
 336:3DPro.c       **** // The ID packet contains: "(ppPNP0F1F\01.00.08)" w/ pp being the encoded PNP
 337:3DPro.c       **** // version no. (1.00)
 338:3DPro.c       **** // QueryFFP() needs to be called before this function for a valid sw_clkcnt.
 339:3DPro.c       **** 
 340:3DPro.c       **** static uint8_t Init3DPro ( void )
 341:3DPro.c       **** {
 342:3DPro.c       ****     if ( ! sw_clkcnt )				// Previous trigger came up empty
 343:3DPro.c       ****     {
 344:3DPro.c       **** 	// Set to digital mode sequence
 345:3DPro.c       **** 
 346:3DPro.c       **** 	dis3DP_INT() ;				// Don't want INT to run..
 347:3DPro.c       **** 
 348:3DPro.c       **** 	Trigger() ;
 349:3DPro.c       **** 
 350:3DPro.c       **** 	Delay_64( T6DEL140US ) ;		// Wait 140us
 351:3DPro.c       **** 
 352:3DPro.c       **** 	Trigger() ;
 353:3DPro.c       **** 
 354:3DPro.c       **** 	Delay_64( (uint8_t)T6DEL865US ) ;	// Wait 140+725us
 355:3DPro.c       **** 
 356:3DPro.c       **** 	Trigger() ;
 357:3DPro.c       **** 
 358:3DPro.c       **** 	Delay_64( T6DEL440US ) ;		// Wait 140+300us
 359:3DPro.c       **** 
 360:3DPro.c       **** 	Trigger() ;
 361:3DPro.c       **** 
 362:3DPro.c       **** 	Delay_1024( T0DEL4MS ) ;		// Hang out 4ms to let everything calm down
 363:3DPro.c       **** 
 364:3DPro.c       **** 						// Try to read a data packet,
 365:3DPro.c       **** 	Query3DP( 0, 126 ) ;			// don't know how long (64 or 66) - let it time out
 366:3DPro.c       **** 
 367:3DPro.c       **** 	if ( ! sw_clkcnt || ! ~sw_clkcnt )	// nothing..
 368:3DPro.c       **** 	    return ( FALSE ) ;
 369:3DPro.c       **** 
 370:3DPro.c       **** 	Delay_1024( T0DEL4MS ) ;		// Another break for the stick
 371:3DPro.c       ****     }
 372:3DPro.c       **** 
 373:3DPro.c       ****     if ( sw_clkcnt == 64 )			// 3DP in 1-bit mode
 374:3DPro.c       ****     {						// Read ID, signal kick at -8 clk of data packet
 375:3DPro.c       **** 						// ID packet is 160 long + 64 or 66
 376:3DPro.c       **** 	Query3DP( -(sw_clkcnt - 8), sw_clkcnt + IDSZ3DP ) ;
 377:3DPro.c       **** 
 378:3DPro.c       **** 	Delay_1024( T0DEL8MS ) ;		// Give the 3DP some rest..
 379:3DPro.c       ****     }
 380:3DPro.c       ****     else
 381:3DPro.c       ****     if ( sw_clkcnt != 66 )			// Not 3DP in 3-bit mode
 382:3DPro.c       **** 	return ( FALSE ) ;
 383:3DPro.c       **** 
 384:3DPro.c       ****     if ( ! Query3DP( 0, DATSZ3DP ) )		// Signal for a regular packet, 22 triplets
 385:3DPro.c       **** 	return ( FALSE ) ;			// Timed out..
 386:3DPro.c       **** 
 387:3DPro.c       ****     if ( Check3DPPkt( sw_packet1 ) )		// Packet 1 CRC checks out
 388:3DPro.c       ****     {
 389:3DPro.c       **** 	Copy3DPData( sw_packet1 ) ;		// Copy data to report buffer
 390:3DPro.c       **** 
 391:3DPro.c       **** 	sw_id = SW_ID_3DP ;			// Found 3DP
 392:3DPro.c       **** 
 393:3DPro.c       **** 	return ( TRUE ) ;
 394:3DPro.c       ****     }
 395:3DPro.c       **** 
 396:3DPro.c       **** // It should work the 1st time, let's save some space.
 397:3DPro.c       **** //
 398:3DPro.c       **** //    Delay_1024( T0DEL500US ) ;		// Wait a bit to have 2nd packet ready
 399:3DPro.c       **** //
 400:3DPro.c       **** //    if ( Check3DPPkt( sw_packet2 ) )		// Packet 2 CRC checks out
 401:3DPro.c       **** //    {
 402:3DPro.c       **** //	Copy3DPData( sw_packet2 ) ;		// Copy data to report buffer
 403:3DPro.c       **** //
 404:3DPro.c       **** //	sw_id = SW_ID_3DP ;			// Found 3DP
 405:3DPro.c       **** //
 406:3DPro.c       **** //	return ( TRUE ) ;
 407:3DPro.c       **** //    }
 408:3DPro.c       **** 
 409:3DPro.c       ****     return ( FALSE ) ;				// Stick not found
 410:3DPro.c       **** }
