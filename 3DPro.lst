 514:3DPro.c       **** // Initialize the hardware
 515:3DPro.c       **** 
 516:3DPro.c       **** void init_hw ( void )
 517:3DPro.c       **** {
 518:3DPro.c       ****     // Power down..
 519:3DPro.c       **** 
 520:3DPro.c       ****     ACSR  = _BV( ACD ) ;			// analog comp
 521:3DPro.c       **** 
 522:3DPro.c       ****   #if defined(__AVR_AT90USBX2__)
 523:3DPro.c       **** 
 524:3DPro.c       ****     PRR0  = _BV( PRSPI ) ;			// SPI
 525:3DPro.c       ****     PRR1  = _BV( PRUSART1 ) ;			// USART1
 526:3DPro.c       **** 
 527:3DPro.c       ****   #elif defined(__AVR_AT90USBX6__)
 528:3DPro.c       **** 
 529:3DPro.c       ****     PRR0 =					// TWI, Tim2, SPI, ADC
 530:3DPro.c       **** 	_BV( PRTWI ) | _BV( PRTIM2 ) | _BV( PRSPI ) | _BV( ADC ) ;
 531:3DPro.c       ****     PRR1 = _BV( PRTIM3 ) | _BV( PRUSART1 ) ;	// Tim3, USART1
 532:3DPro.c       **** 
 533:3DPro.c       ****   #elif defined(__AVR_ATmegaXU4__)
 534:3DPro.c       **** 
 535:3DPro.c       ****     PRR0  =					// TWI, SPI, ADC
 536:3DPro.c       **** 	_BV( PRTWI ) | _BV( PRSPI ) | _BV( ADC ) ;
 537:3DPro.c       ****     PRR1  =					// Tim4, Tim3, USART1
 538:3DPro.c       **** 	_BV( PRTIM4 ) | _BV( PRTIM3 ) | _BV( PRUSART1 ) ;
 539:3DPro.c       ****     DIDR1 = _BV( AIN0D ) ;			// Disable digital input buffer
 540:3DPro.c       ****   #endif
 541:3DPro.c       **** 
 542:3DPro.c       ****     // Initialize ports
 543:3DPro.c       **** 
 544:3DPro.c       ****   #if defined(__AVR_AT90USBX6__)
 545:3DPro.c       **** 
 546:3DPro.c       ****     DDRA  = DDA ;
 547:3DPro.c       ****     PORTA = PAPU ;
 548:3DPro.c       ****   #endif
 549:3DPro.c       **** 
 550:3DPro.c       ****     DDRB  = DDB ;
 551:3DPro.c       ****     PORTB = PBPU ;
 552:3DPro.c       ****     DDRC  = DDC ;
 553:3DPro.c       ****     PORTC = PCPU ;
 554:3DPro.c       ****     DDRD  = DDD ;
 555:3DPro.c       ****     PORTD = PDPU ;
 556:3DPro.c       **** 
 557:3DPro.c       ****   #if defined(__AVR_ATmegaXU4__) || defined(__AVR_AT90USBX6__)
 558:3DPro.c       **** 
 559:3DPro.c       ****     DDRE  = DDE ;
 560:3DPro.c       ****     PORTE = PEPU ;
 561:3DPro.c       ****     DDRF  = DDF ;
 562:3DPro.c       ****     PORTF = PFPU ;
 563:3DPro.c       ****   #endif
 564:3DPro.c       **** 
 565:3DPro.c       ****     clock_prescale_set( clock_div_1 ) ;		// Set clock divider to 1, full speed
 566:3DPro.c       **** 
 567:3DPro.c       ****     // Initialize timers
 568:3DPro.c       **** 
 569:3DPro.c       ****     SetTMPS( 0, 64 ) ;				// Set T0 prescaler to / 64 for query & us delay
 570:3DPro.c       ****     SetTMPS( 1, 1024 ) ;			// Set T1 prescaler to / 1024 for ms delay
 571:3DPro.c       **** 
 572:3DPro.c       ****     EICRA  = _B1(ISC01) | _B1(ISC00) ;		// Need INT0 on rising edges
 573:3DPro.c       **** 
 574:3DPro.c       ****     Delay_1024( T0DEL200MS ) ;			// Allow the stick to boot
 575:3DPro.c       **** 
 576:3DPro.c       ****     LED_off() ;
 577:3DPro.c       **** 						// Hold 3 & 4, then switch from * --> **
 578:3DPro.c       ****     if ( (BUTPIN & BUTMSK) == (_B1(BUT1) | _B1(BUT2) | _B0(BUT3) | _B0(BUT4)) )
 579:3DPro.c       **** 	jmp_bootloader() ;
 580:3DPro.c       **** 
 581:3DPro.c       **** //    if ( ClrSerial() )			// Clear serial no. condition met ?
 582:3DPro.c       **** //	eeprom_write_byte( NULL, 0xFF ) ;	// Invalidate serial no.
 583:3DPro.c       **** 
 584:3DPro.c       ****     for ( ;; )					// Forever..
 585:3DPro.c       ****     {
 586:3DPro.c       **** 	Flash_LED_12MS() ;			// Flash LED and wait
 587:3DPro.c       **** 	Delay_1024( T0DEL200MS ) ;
 588:3DPro.c       **** 						// Try to read a data packet,
 589:3DPro.c       **** 	QueryFFP( 0, 126 ) ;			// don't know how long - let it time out
 590:3DPro.c       **** 
 591:3DPro.c       **** 	Flash_LED_12MS() ;
 592:3DPro.c       **** 
 593:3DPro.c       **** 	// Analyze clock count
 594:3DPro.c       **** 
 595:3DPro.c       **** 	if ( ! ~sw_clkcnt )
 596:3DPro.c       **** 	    sw_clkcnt = 0 ;
 597:3DPro.c       **** 
 598:3DPro.c       **** 	if (    sw_clkcnt == (16 + 1)		// FFP/PP in 3-bit mode
 599:3DPro.c       **** 	     || sw_clkcnt == (48 + 1) )		// 3DPP in 1-bit mode
 600:3DPro.c       **** 	{
 601:3DPro.c       **** 	    if ( InitFFPro() )			// found FFP/PP
 602:3DPro.c       **** 		break ;				// break forever
 603:3DPro.c       **** 	}
 604:3DPro.c       **** 	else
 605:3DPro.c       **** 	if ( Init3DPro() )			// Check for 3DP
 606:3DPro.c       **** 	{					// found 3DP
 607:3DPro.c       **** 	  #if SIXAXIS
 608:3DPro.c       **** 
 609:3DPro.c       **** 	    clr_bit( PRR0, ADC ) ;		// power up ADC
 610:3DPro.c       **** 
 611:3DPro.c       **** 	    set_sleep_mode( SLEEP_MODE_ADC ) ;	// Works w/ USB module ? Seems to.
 612:3DPro.c       **** 
 613:3DPro.c       **** 	    Delay_1024( T0DEL4MS ) ;		// Wait for all activity to stop
 614:3DPro.c       **** 
 615:3DPro.c       **** 	    clr_bits( PORTF, _B1(PF4) | _B1(PF5) ) ;// Turn off analog pin pull-up's
 616:3DPro.c       **** 
 617:3DPro.c       **** 	    DIDR0 = _B1(ADC5D) | _B1(ADC4D) ;	// Disable digital input buffers
 618:3DPro.c       **** 
 619:3DPro.c       **** 	    ADMUX  =
 620:3DPro.c       **** 		_B0(REFS1) | _B1(REFS0) |       // AVcc w/ cap on AREF
 621:3DPro.c       **** 		_B1(ADLAR) |                    // left adjusted result (!)
 622:3DPro.c       **** 		4 ;                             // Preselect channel 4
 623:3DPro.c       **** 
 624:3DPro.c       **** 	    ADCSRA =				// Initialize ADC for Rx/Ry
 625:3DPro.c       **** 		_B1(ADEN) |			//   enable
 626:3DPro.c       **** 		_B0(ADSC) |			// ! start conversion
 627:3DPro.c       **** 		_B0(ADATE) |			// ! auto trigger enable
 628:3DPro.c       **** 		_B1(ADIF) |			//   clear interrupt flag
 629:3DPro.c       **** 		_B1(ADIE) |			//   enable interrupt
 630:3DPro.c       **** 		_B0(ADPS2) | _B1(ADPS1) | _B1(ADPS0) ; // prescaler / 8 --> 125kHz
 631:3DPro.c       **** 
 632:3DPro.c       **** 	    ReadADC( ADC_CRX ) ;		// Start up ADC, read AcntrX
 633:3DPro.c       **** 	    ReadADC( ADC_CRY ) ;		// Read AcntrY
 634:3DPro.c       **** 
 635:3DPro.c       **** //	    if ( sw_packet1[3] & 0x20 )		// B9 in pos. II, could sig. calibrate
 636:3DPro.c       **** //	    {
 637:3DPro.c       **** //	    }
 638:3DPro.c       **** 	  #endif
 639:3DPro.c       **** 
 640:3DPro.c       **** 	    break ;				// break forever
 641:3DPro.c       **** 	}
 642:3DPro.c       **** 
 643:3DPro.c       **** 	dis3DP_INT() ;
 644:3DPro.c       ****     }
 645:3DPro.c       **** 
 646:3DPro.c       ****     dis3DP_INT() ;				// Disable INT
 647:3DPro.c       **** 
 648:3DPro.c       ****     cli() ;					// Disable interrupts
 649:3DPro.c       **** 
 650:3DPro.c       ****     sw_buttons = BUTMSK ;			// All buttons released
 651:3DPro.c       **** }
