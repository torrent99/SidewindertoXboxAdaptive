 214:3DPro.c       **** 
 215:3DPro.c       **** // Check if the FFP/PP data packet at *pkt is valid by xoring all bits together.
 216:3DPro.c       **** // Result must be 1 for the packet to have correct parity.
 217:3DPro.c       **** 
 218:3DPro.c       **** static uint8_t FA_NOINLINE( CheckFFPPkt ) ( uint8_t *pkt )
 219:3DPro.c       **** {
 220:3DPro.c       ****     uint8_t
 221:3DPro.c       **** 	x ;
 222:3DPro.c       **** 
 223:3DPro.c       ****     x  = pkt[0] ^ pkt[1] ^ pkt[2] ^ pkt[3] ^ pkt[4] ^ pkt[5] ;
 224:3DPro.c       **** 
 225:3DPro.c       ****     x ^= x >> 4 ;
 226:3DPro.c       ****     x ^= x >> 2 ;
 227:3DPro.c       ****     x ^= x >> 1 ;
 228:3DPro.c       **** 
 229:3DPro.c       ****     return ( x & 1 ) ;
 230:3DPro.c       **** }
 231:3DPro.c       **** 
 232:3DPro.c       **** //------------------------------------------------------------------------------
 233:3DPro.c       **** 
 234:3DPro.c       **** // Check if the 3DPro data packet at *pkt is valid by adding all nibbles
 235:3DPro.c       **** // (result must be 0) and checking the N bits of each byte.
 236:3DPro.c       **** 
 237:3DPro.c       **** static uint8_t FA_NOINLINE( Check3DPPkt ) ( uint8_t *pkt )
 238:3DPro.c       **** {
 239:3DPro.c       ****     uint8_t
 240:3DPro.c       **** 	b,
 241:3DPro.c       **** 	i   = 8,				// 8 bytes
 242:3DPro.c       **** 	sum = 0 ;				// clear CHKSUM
 243:3DPro.c       **** 
 244:3DPro.c       ****     do						// for every byte
 245:3DPro.c       ****     {
 246:3DPro.c       **** 	b = *pkt++ ;
 247:3DPro.c       **** 
 248:3DPro.c       **** //	To speed things up let's drop the N flag test. CHKSUM should be enough.
 249:3DPro.c       **** //
 250:3DPro.c       **** //	if (   (b & 0x80)			// N flag set ?
 251:3DPro.c       **** //	     && i != 1 )			// Not last byte ?
 252:3DPro.c       **** //	    return ( 0 ) ;			// bad packet
 253:3DPro.c       **** 
 254:3DPro.c       **** 	sum += b ;				// add LSN
 255:3DPro.c       **** 
 256:3DPro.c       **** 	__asm__ __volatile__
 257:3DPro.c       **** 	(
 258:3DPro.c       **** 	    "swap %0\n\t" : "+r" (b) : "0" (b)
 259:3DPro.c       **** 	) ;
 260:3DPro.c       **** 
 261:3DPro.c       **** 	sum += b ;				// add MSN
 262:3DPro.c       ****     }
 263:3DPro.c       ****     while ( --i ) ;
 264:3DPro.c       **** 
 265:3DPro.c       ****     return ( ! (sum & 0x0F) ) ;			// good if sum of all nibbs == 0
 266:3DPro.c       **** }
