 430:3DPro.c       **** #if SIXAXIS
 431:3DPro.c       **** 
 432:3DPro.c       **** // Need the ADC ISR handler for wakeup from sleep.
 433:3DPro.c       **** 
 434:3DPro.c       **** EMPTY_INTERRUPT( ADC_vect ) 
 435:3DPro.c       **** 
 436:3DPro.c       **** //------------------------------------------------------------------------------
 437:3DPro.c       **** 
 438:3DPro.c       **** #define	ADC_RX		0b0100
 439:3DPro.c       **** #define	ADC_RY		0b0101
 440:3DPro.c       **** 
 441:3DPro.c       **** #define	ADC_CRX		0b1100
 442:3DPro.c       **** #define	ADC_CRY		0b1101
 443:3DPro.c       **** 
 444:3DPro.c       **** static void ReadADC ( uint8_t chan )
 445:3DPro.c       **** {
 446:3DPro.c       ****     uint8_t
 447:3DPro.c       **** 	adc8 ;
 448:3DPro.c       ****     uint16_t
 449:3DPro.c       **** 	adc16 ;
 450:3DPro.c       **** 
 451:3DPro.c       ****     if ( chan & 0x01 )
 452:3DPro.c       **** 	set_bit( ADMUX, MUX0 ) ;		// select channel 5, Ry
 453:3DPro.c       ****     else
 454:3DPro.c       **** 	clr_bit( ADMUX, MUX0 ) ;		// select channel 4, Rx
 455:3DPro.c       **** 
 456:3DPro.c       ****     sleep_enable() ;
 457:3DPro.c       **** 
 458:3DPro.c       ****     do
 459:3DPro.c       **** 	sleep_cpu() ;				// ADCNR/IDLE sleep, starts conv.
 460:3DPro.c       ****     while ( bit_is_set( ADCSRA, ADSC ) ) ;
 461:3DPro.c       **** 
 462:3DPro.c       ****     sleep_disable() ;
 463:3DPro.c       **** 
 464:3DPro.c       ****     adc16 = ADC ;				// Read A value, left adj. (!)
 465:3DPro.c       **** 
 466:3DPro.c       ****     switch ( chan )
 467:3DPro.c       ****     {
 468:3DPro.c       **** 	case ADC_RX :
 469:3DPro.c       **** 	    if ( adc16 >= AcntrX )
 470:3DPro.c       **** 		adc8 = -((adc16 - AcntrX) >> 8) ;
 471:3DPro.c       **** 	    else
 472:3DPro.c       **** 	    {
 473:3DPro.c       **** 		adc8 = (AcntrX - adc16) >> 8 ;
 474:3DPro.c       **** 
 475:3DPro.c       **** 		if ( adc8 > 0x7F )
 476:3DPro.c       **** 		    adc8 = 0x7F ;
 477:3DPro.c       **** 	    }
 478:3DPro.c       **** 
 479:3DPro.c       **** 	    sw_report[2] = (sw_report[2] & 0x0F) | (adc8 << 4) ;
 480:3DPro.c       **** 	    sw_report[3] = (sw_report[3] & 0xF0) | (adc8 >> 4) ;
 481:3DPro.c       **** 
 482:3DPro.c       **** 	    break ;
 483:3DPro.c       **** 
 484:3DPro.c       **** 	case ADC_RY :
 485:3DPro.c       **** 	    if ( adc16 >= AcntrY )
 486:3DPro.c       **** 		adc8 = -((adc16 - AcntrY) >> 8) ;
 487:3DPro.c       **** 	    else
 488:3DPro.c       **** 	    {
 489:3DPro.c       **** 		adc8 = (AcntrY - adc16) >> 8 ;
 490:3DPro.c       **** 
 491:3DPro.c       **** 		if ( adc8 > 0x7F )
 492:3DPro.c       **** 		    adc8 = 0x7F ;
 493:3DPro.c       **** 	    }
 494:3DPro.c       **** 
 495:3DPro.c       **** 	    sw_report[3] = (sw_report[3] & 0x0F) | (adc8 << 4) ;
 496:3DPro.c       **** 	    sw_report[4] = (sw_report[4] & 0xF0) | (adc8 >> 4) ;
 497:3DPro.c       **** 
 498:3DPro.c       **** 	    break ;
 499:3DPro.c       **** 
 500:3DPro.c       **** 	case ADC_CRX :			// center Rx
 501:3DPro.c       **** 	    AcntrX = adc16 ;
 502:3DPro.c       **** 	    break ;
 503:3DPro.c       **** 
 504:3DPro.c       **** 	case ADC_CRY :			// center Ry
 505:3DPro.c       **** 	    AcntrY = adc16 ;
 506:3DPro.c       **** 	    break ;
 507:3DPro.c       ****     }
 508:3DPro.c       **** }
 509:3DPro.c       **** 
 510:3DPro.c       **** #endif
